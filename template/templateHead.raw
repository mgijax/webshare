<script>
function addTitleBarIcon()
{
    // pull in the title bar, and gather needed information
    var titleBar = document.getElementById("titleBar");
    var userdoc = titleBar.getAttribute("userdoc");
    var currentHTML = titleBar.innerHTML;
    var helpIcon = '<img class="noBorder" src="http://firien.informatics.jax.org/usrlocalmgi/live/webshare/images/help_large_transp.gif">';
    var helpLink = '';

    if (userdoc != null & userdoc !="") {
        helpLink = "<a class='titleBarHelpIcon' href='http://firien.informatics.jax.org/usrlocalmgi/live/userdocs/www/" + userdoc + "'>" + helpIcon + "</a>";
        titleBar.innerHTML = helpLink + currentHTML;
    }
};

</script>
