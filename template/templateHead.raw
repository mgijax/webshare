<script>
function loadPage()
{
    // pull in the title bar, and gather needed information
    var titleBar = document.getElementById("titleBar");
    var userdoc = titleBar.getAttribute("userdoc");
    var currentHTML = titleBar.innerHTML;
    var helpIcon = '<img class="noBorder" src="${WEBSHARE_URL}images/help_large_transp.gif">';
    var helpLink = '';

    if (userdoc != null & userdoc !="") {
        helpLink = "<a class='titleBarHelpIcon' href='${USERDOCS_URL}" + userdoc + "'>" + helpIcon + "</a>";
        titleBar.innerHTML = helpLink + currentHTML;
    }
};

</script>
